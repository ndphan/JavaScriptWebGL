{
  "name": "synaren-engine",
  "version": "0.0.2",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/",
    "package.json"
  ],
  "scripts": {
    "start": "webpack serve --mode development --config webpack.config.js",
    "dev": "webpack serve --mode development --config webpack.config.example.js",
    "lint": "eslint --fix --ignore-pattern node_modules/ --ext .ts src/",
    "check-duplicates": "jscpd --reporters console,html,json --min-lines 5 src/",
    "quality:check": "yarn tsc --noEmit && yarn lint --max-warnings=0 && yarn check-duplicates && yarn test:coverage",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "lint:staged": "lint-staged",
    "agent:run": "yarn quality:check",
    "build": "tsc --outDir dist --declaration --emitDeclarationOnly false && webpack --mode production --config webpack.config.js",
    "build:dev": "webpack --mode development --config webpack.config.js",
    "build:example": "webpack --mode production --config webpack.config.example.js",
    "serve:example": "webpack serve --mode development --config webpack.config.example.js",
    "link": "yarn build && yarn link",
    "unlink": "yarn unlink",
    "test": "jest --runInBand",
    "test:watch": "jest --watch --runInBand",
    "test:coverage": "jest --coverage --runInBand",
    "test:verbose": "jest --verbose --runInBand"
  },
  "dependencies": {
    "gl-matrix": "^3.4.3"
  },
  "devDependencies": {
    "@babel/core": "^7.21.3",
    "@babel/plugin-transform-runtime": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/runtime": "^7.21.0",
    "@types/gl": "^6.0.5",
    "@types/gl-matrix": "^3.2.0",
    "@types/jest": "latest",
    "@types/minimatch": "^6.0.0",
    "@types/node": "^18.15.3",
    "@types/webgl2": "0.0.7",
    "@types/webpack": "^5.28.0",
    "babel-loader": "^9.1.2",
    "canvas": "^3.2.0",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^7.6.0",
    "copy-webpack-plugin": "^13.0.0",
    "css-loader": "^6.7.3",
    "eslint": "^8.36.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-sonarjs": "^0.17.0",
    "file-loader": "^6.2.0",
    "jest": "latest",
    "jest-environment-jsdom": "latest",
    "jest-puppeteer": "^11.0.0",
    "jest-webgl-canvas-mock": "^2.5.3",
    "jscpd": "^3.4.0",
    "lint-staged": "^14.0.0",
    "postcss-preset-env": "^8.5.1",
    "prettier": "^2.8.4",
    "prettier-eslint": "^15.0.1",
    "puppeteer": "^24.34.0",
    "raw-loader": "^4.0.2",
    "style-loader": "^3.3.2",
    "ts-jest": "latest",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.2",
    "typescript-eslint": "^8.38.0",
    "typescript-tslint-plugin": "^1.0.2",
    "webpack": "^5.76.2",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^5.2.2",
    "webpack-glob": "^2.0.2",
    "wscat": "^5.2.0"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx,json,md}": [
      "prettier --write",
      "eslint --fix --ext .ts,.tsx,.js,.jsx"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
